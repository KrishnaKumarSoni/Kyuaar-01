{
  "version": 2,
  "name": "kyuaar-01",
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "wsgi.py": {
      "maxDuration": 30
    }
  }
}